    "# sum of 1 to n natural number :\n",
    "\n",
    "no = int(input(\"Input a number which you want to print sum : \"))\n",
    "sum = 0\n",
    "\n",
    "while no >=0:\n",
    "    sum = sum + no\n",
    "    no = no - 1\n",
    "print(f\"Sum = {sum}\")"
   ]
